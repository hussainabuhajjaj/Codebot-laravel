{
    "name": "hussainabuhajjaj/codebot-laravel",
    "description": "AI-powered Laravel component generator",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.7",
        "ext-json": "*"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hussainabuhajjaj\\Codebot\\CodebotServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}