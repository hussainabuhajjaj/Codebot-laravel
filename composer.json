{
    "name": "hussainabuhajjaj/codebot-laravel",
    "description": "AI-powered Laravel component generator",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.7",
        "ext-json": "*"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hussainabuhajjaj\\Codebot\\CodebotServiceProvider"
            ],
            "dont-discover": []
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
