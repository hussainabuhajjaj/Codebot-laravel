{
    "name": "hussainabuhajjaj/codebot-laravel",
    "description": "AI-powered Laravel component generator",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hussainabuhajjaj\\Codebot\\CodebotServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}