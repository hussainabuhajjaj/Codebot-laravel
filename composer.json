{
    "name": "hussainabuhajjaj/codebot-laravel",
    "description": "A Laravel package to generate migrations, models, and views using DeepSeek AI.",
    "type": "library",
    "keywords": ["laravel", "codebot", "migrations", "models", "views", "bootstrap", "tailwind", "ai"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hussain Abuhajjaj",
            "email": "your-email@example.com",
            "homepage": "https://github.com/hussainabuhajjaj",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hussainabuhajjaj\\Codebot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hussainabuhajjaj\\Codebot\\CodebotServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}